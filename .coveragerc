[run]
source =
  chess
  main
  board
  rook
  horse
  pawn
  bishop
  king
  queen
omit =
    venv/*
    */test*
    */__init__.py
[report]
exclude_lines =
    pragma: no cover
    def setUp
    def test_
    return raw_input
    print
    if __name__ == '__main__':
    def suite
    __str__
    main()